<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane fx:id="mainPane" prefHeight="245.0" prefWidth="404.0">
            <children>
                <Label layoutX="100.0" layoutY="14.0" text="Учет инвентаря на складах предприятиях"/>

                      <Button fx:id="inventoryOperationButton" layoutX="100.0" layoutY="40.0" mnemonicParsing="false" text="Операции над инвентарем" />
                      <Button fx:id="storageOperationButton" layoutX="100.0" layoutY="70.0" mnemonicParsing="false" text="Операции над складами" />
                      <Button fx:id="invoiceOperationButton" layoutX="100.0" layoutY="100.0" mnemonicParsing="false" text="Операции над накладными" />
                      <Button fx:id="formButton" layoutX="100.0" layoutY="130.0" mnemonicParsing="false" text="Поиск" />
              </children></Pane>
      </children>
  </GridPane>
