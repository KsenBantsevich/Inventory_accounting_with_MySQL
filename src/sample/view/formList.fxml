<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.FormListController">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane fx:id="mainPane" prefHeight="326.0" prefWidth="564.0">
            <children>
                <TableView fx:id="formTableView" layoutX="5.0" layoutY="34.0" prefHeight="200.0" prefWidth="565.0">
                    <columns>
                        <TableColumn fx:id="formNumberColumn" prefWidth="90.0" text="Код формы" />
                        <TableColumn fx:id="storageNumberColumn" prefWidth="95.0" text="Код склада" />
                        <TableColumn fx:id="transactionDateColumn" prefWidth="110" text="Дата совершения" />
                        <TableColumn fx:id="employeeFioColumn" prefWidth="120.0" text="ФИО работника" />
                        <TableColumn fx:id="employeePostColumn" prefWidth="150.0" text="Должность работника" />
                    </columns>
                </TableView>
                <Button fx:id="inventorySearchButton" layoutX="5.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="116.0" text="Поиск инвентаря" />
                <Button fx:id="invoiceSearchButton" layoutX="235.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="116.0" text="Поиск накладных" />
                <Button fx:id="storageSearchButton" layoutX="458.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="116.0" text="Поиск складов" />
                <Button fx:id="cancelButton" layoutX="270.0" layoutY="287.0" mnemonicParsing="false" text="Назад" />
            </children></Pane>
    </children>
</GridPane>
