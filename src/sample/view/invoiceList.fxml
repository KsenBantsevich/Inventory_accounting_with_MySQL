<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.InvoiceListController">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane fx:id="mainPane" prefHeight="326.0" prefWidth="564.0">
            <children>
                <TableView fx:id="invoiceTableView" layoutX="25.0" layoutY="34.0" prefHeight="200.0" prefWidth="530.0">
                    <columns>
                        <TableColumn fx:id="invoiceNumberColumn" prefWidth="130.0" text="Код накладной" />
                        <TableColumn fx:id="formNumberColumn" prefWidth="120.0" text="Код формы" />
                        <TableColumn fx:id="inventoryNumberColumn" prefWidth="130.0" text="Код инвентаря" />
                        <TableColumn fx:id="inventoryAmountColumn" prefWidth="150.0" text="Количество инвентаря" />
                    </columns>
                </TableView>
                <Button fx:id="addButton" layoutX="25.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="110.0" text="Добавить" />
                <Button fx:id="editButton" layoutX="240.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="110.0" text="Редактировать" />
                <Button fx:id="deleteButton" layoutX="446.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="110.0" text="Удалить" />
                <Button fx:id="cancelButton" layoutX="270.0" layoutY="287.0" mnemonicParsing="false" text="Назад" />
            </children></Pane>
    </children>
</GridPane>
